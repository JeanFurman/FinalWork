@model Cliente

<div class="container">
    <form asp-action="Cadastrar">
        <fieldset>
            <legend>Cadastrar Cliente</legend>
            <div asp-validation-summary="ModelOnly" class="text-warning"></div>
            <div class="form-group" id="Nome">
                <label asp-for="Nome"></label>
                <input type="text" asp-for="Nome" class="form-control" />
                <span asp-validation-for="Nome" class="text-warning"></span>
            </div>
            <div class="form-group" id="Cpf">
                <label asp-for="Cpf"></label>
                <input type="text" asp-for="Cpf" class="form-control" />
                <span asp-validation-for="Cpf" class="text-warning"></span>

            </div>
            <div class="form-group" id="DataNasc">
                <label asp-for="DataNasc"></label>
                <input type="date" asp-for="DataNasc" class="form-control" />
                <span asp-validation-for="DataNasc" class="text-warning"></span>

            </div>
            <div class="form-group" id="Contato">
                <label asp-for="Contato"></label>
                <input type="text" asp-for="Contato" class="form-control" />
                <span asp-validation-for="Contato" class="text-warning"></span>

            </div>
            <input type="submit" class="btn btn-primary" value="Cadastrar" />
        </fieldset>
    </form>
</div>


