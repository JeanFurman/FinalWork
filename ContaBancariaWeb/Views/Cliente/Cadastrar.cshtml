@model Cliente

<div class="container">
    <form asp-action="Cadastrar">
        <fieldset>
            <legend>Cadastrar Cliente</legend>
            <div asp-validation-summary="ModelOnly" class="text-warning"></div>
            <div class="form-group" id="Nome">
                <label asp-for="Nome_da_Pf"></label>
                <input type="text" asp-for="Nome_da_Pf" class="form-control" readonly=""/>
                <span asp-validation-for="Nome_da_Pf" class="text-warning"></span>
            </div>
            <div class="form-group" id="Cpf">
                <label asp-for="Numero_de_Cpf"></label>
                <input type="text" asp-for="Numero_de_Cpf" class="form-control" />
                <span asp-validation-for="Numero_de_Cpf" class="text-warning"></span>

            </div>
            <div class="form-group" id="DataNasc">
                <label asp-for="Data_Nascimento"></label>
                <input type="date" asp-for="Data_Nascimento" class="form-control" />
                <span asp-validation-for="Data_Nascimento" class="text-warning"></span>

            </div>
            <input type="submit" class="btn btn-primary" value="Buscar" asp-action="BuscarCpf"/>
            <div class="form-group" id="Contato">
                <label asp-for="Contato"></label>
                <input type="text" asp-for="Contato" class="form-control" />
                <span asp-validation-for="Contato" class="text-warning"></span>

            </div>
            <input type="submit" class="btn btn-primary" value="Cadastrar" />
        </fieldset>
    </form>
</div>


